@page
@model Starbender.Romi.WebApi.Areas.Administration.Pages.ConfigurationModel
@{
    Layout = "/Views/Shared/_Layout.cshtml";
}

<body>
<h4>
    <small>Current Settings</small>
</h4>
<table class="table table-striped table-bordered">
    <thead>
    <tr>
        <th>Data</th>
        <th>Value</th>
    </tr>
    </thead>
    <tbody>

    @foreach (var name in Model.Settings.Keys)
    {
        <tr>
            <td>@name</td>
            <td>@Model.Settings[name]</td>
        </tr>
    }
    </tbody>
</table>
</body>